{
  "manifest_version": 3,
  "name": "vkAudio",
  "description": "Скачивай музыку Вконтакте в один клик!",
  "version": "1.0",
  "icons": {
    "128": "vkAudio_icon_128.png",
    "96": "vkAudio_icon_96.png",
    "64": "vkAudio_icon_64.png",
    "48": "vkAudio_icon_48.png",
    "32": "vkAudio_icon_32.png",
    "24": "vkAudio_icon_24.png",
    "16": "vkAudio_icon_16.png"
  },
  "content_scripts": [
    {
      "matches": ["http://vk.com/*", "https://vk.com/*"],
      "css": ["main.css"],
      "js": ["vkAudio_index.js"]
    }
  ]
}
